{
  "name": "typescript-nextjs-starter",
  "description": "NextJS + Keystone Blog",
  "version": "0.0.1",
  "private": true,
  "author": "Johan Petrikovsky (@jkovsky)",
  "license": "MIT",
  "keywords": [
    "nextjs",
    "starter",
    "typescript"
  ],
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "keystone-next postinstall",
    "commit": "cz",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ],
    "*.js": "eslint --cache --fix"
  },
  "dependencies": {
    "@emotion/react": "11.4.0",
    "@emotion/styled": "11.3.0",
    "@keystone-next/fields": "13.0.0",
    "@keystone-next/fields-document": "7.0.3",
    "@keystone-next/keystone": "23.0.0",
    "moment": "2.29.1",
    "next": "11.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-markdown": "6.0.2",
    "react-moment": "1.1.1",
    "storybook-addon-styled-component-theme": "2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@react-theming/storybook-addon": "1.1.1",
    "@storybook/addon-actions": "^6.3.5",
    "@storybook/addon-essentials": "^6.3.5",
    "@storybook/addon-links": "^6.3.5",
    "@storybook/react": "^6.3.5",
    "@types/node": "14.17.5",
    "@types/react": "17.0.14",
    "@types/react-dom": "17.0.9",
    "@typescript-eslint/eslint-plugin": "4.28.3",
    "@typescript-eslint/parser": "4.28.3",
    "babel-loader": "^8.2.2",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.30.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-next": "11.0.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "^7.0.0",
    "lint-staged": "11.0.1",
    "prettier": "2.3.2",
    "typescript": "4.3.5"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
